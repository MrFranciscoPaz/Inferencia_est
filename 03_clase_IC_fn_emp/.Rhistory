library(pander) #Paquetería para sacar tablas más bonitas
library(tidyverse)
f_empirica <- function(x, y){
n <- length(x)
sum <- 0
for (i in 1:length(x)) {
if(x[i] <= y){
sum = sum + 1
}
}
sum/n
}
Int_inf <- function(x,y,a){
res <- -100
n <- length(x)
e <- sqrt((1/(2*n))*log(2/a))
val <- f_empirica(x,y)-e
if(val < 0) res <- 0
if(val >= 0) res <- val
return(res)
}
Int_sup <- function(x,y,a){
res <- -100
n <- length(x)
e <- sqrt((1/(2*n))*log(2/a))
val <- f_empirica(x,y)+e
if(val < 1) res <- val
if(val >= 1) res <- 1
return(res)
}
